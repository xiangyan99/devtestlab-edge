{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "labResourceGroupName": {
            "type": "string"
        },
        "labVirtualNetworkName": {
            "type": "string"
        },
        "clientAddressPool": {
            "type": "string"
        },
        "clientRootCertName": {
            "type": "string"
        },
        "clientRootCertData": {
            "type": "string"
        },
        "revokedClientThumbprints": {
            "type": "string"
        },
        "_artifactsLocation": {
            "type": "string",
            "metadata": {
                "description": "The base URI where artifacts required by this template are located. When the template is deployed using the accompanying scripts, a private location in the subscription will be used and this value will be automatically generated."
            },
            "defaultValue": "https://raw.githubusercontent.com/markusheiliger/devtestlab-edge/master/environments/VPN-Gateway"
        },
        "_artifactsLocationSasToken": {
            "type": "securestring",
            "metadata": {
                "description": "The sasToken required to access _artifactsLocation.  When the template is deployed using the accompanying scripts, a sasToken will be automatically generated."
            },
            "defaultValue": ""
        }
    },
    "variables": {
    },
    "resources": [
        {
            "name": "[concat(deployment().name, '-Internal')]",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "resourceGroup": "[parameters('labResourceGroupName')]",
            "properties": {
                "mode": "Incremental",
                "validationLevel": "Template",
                "templateLink": {
                    "uri": "[concat(parameters('_artifactsLocation'), '/101-acs-kubernetes/azuredeploy.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "labResourceGroupName": {
                        "value": "[parameters('labResourceGroupName')]"
                    },
                    "labVirtualNetworkName": {
                        "value": "[parameters('labVirtualNetworkName')]"
                    },
                    "clientAddressPool": {
                        "value": "[parameters('clientAddressPool')]"
                    },
                    "clientRootCertName": {
                        "value": "[parameters('clientRootCertName')]"
                    },
                    "clientRootCertData": {
                        "value": "[parameters('clientRootCertData')]"
                    },
                    "revokedClientThumbprints": {
                        "value": "[parameters('revokedClientThumbprints')]"
                    },
                    "_artifactsLocation": {
                        "value": "[parameters('_artifactsLocation')]"
                    },
                    "_artifactsLocationSasToken": {
                        "value": "[parameters('_artifactsLocationSasToken')]"
                    }
                }
            }
        }
    ]
}